{
    "name": "NeurAlert",
    
    "version": "1.0",
    
    "description": "For Early Detection of Neurological Impairment",

    "browser_action": {
      "default_icon": "icon.png"
    },
    
    "manifest_version": 2,
    
    "incognito": "split",
    
    "permissions": [
       "activeTab"
     ],

    "content_scripts": [
    {
        "matches": [
        "<all_urls>"
        ],
        "js": ["main.js"]
    }
    ]
  }